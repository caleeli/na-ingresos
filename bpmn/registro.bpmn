      <script><![CDATA[<?php

use Na\Ingresos\Models\Recibo;

$data = $model->data->recibo;
$data->egreso = $data->tipo === 'egreso' ? $data->monto : 0;
$data->ingreso = $data->tipo === 'ingreso' ? $data->monto : 0;
$data->egreso_acumulado = Recibo::sum('egreso') + $data->egreso;
